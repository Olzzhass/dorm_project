<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project_inf201.StudentHomePageController">
   <center>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="508.0" prefWidth="900.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="508.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/StudentHomePage.png" />
               </image>
            </ImageView>
            <Button fx:id="NextButton" layoutX="138.0" layoutY="401.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="106.0" style="-fx-background-color: #9BE6FD; -fx-background-radius: 50;" text="Next" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Button>
            <ComboBox fx:id="Rooms" layoutX="76.0" layoutY="340.0" prefHeight="31.0" prefWidth="230.0" promptText="Rooms" style="-fx-background-radius: 50; -fx-background-color: #FFFFFF; -fx-border-color: #B6B6B6; -fx-border-radius: 50;" />
            <Label fx:id="StudentName" layoutX="66.0" layoutY="194.0" prefHeight="20.0" prefWidth="250.0" text="Student : ">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="UniversityName" layoutX="66.0" layoutY="224.0" prefHeight="20.0" prefWidth="250.0" text="University : ">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="RoomNumber" layoutX="66.0" layoutY="254.0" prefHeight="9.0" prefWidth="250.0" text="Room number : ">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="BackButton" layoutX="802.0" layoutY="8.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="90.0" style="-fx-background-color: #9BE6FD; -fx-background-radius: 50;" text="Back" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
